@model List<gnk_invest_test.Models.Sale>

<table>
    <thead>
        <tr>
            <th>Id из сделки</th>
            <th>Название сделки</th>
            <th>Id контрагента</th>
            <th>Название контрагента</th>
            <th>Адрес регистрации</th>
            <th>Почтовый адрес</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var sale in Model)
        {
            <tr>
                <td>@sale.Id</td>
                <td>@sale.Name</td>
                <td>@sale.Contractor.Id</td>
                <td>@sale.Contractor.Name</td>
                <td>@sale.Contractor.LegalAddress</td>
                <td>@sale.Contractor.PostalAddress</td>
            </tr>
        }
    </tbody>
</table>
